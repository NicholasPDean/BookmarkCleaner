{
"name": "Bookmark Cleaner", //required
"version": "1.0", //required
"description": "Tool to delete empty bookmark folders!",
"manifest_version": 2,//requireds
"icons":
	{
		"16": "images/16px.png", 
		"48": "images/48px.png",
		"128": "images/128px.png"
	},
"browser_action":
	{
		"default_icon": "images/original.png"
	},

"permissions": [
	"bookmarks",
	"tabs",
	"unlimitedStorage",
	"activeTab"
	],
"background": 
	{
		"persistent": false,
        "scripts": [
            "background.js" 
        ]
    }
}